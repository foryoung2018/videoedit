<resources xmlns:tools="http://schemas.android.com/tools">

    <!--基本的主题-->
    <style name="AppTheme.base" parent="Theme.AppCompat.Light">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <!--状态栏颜色-->
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <!--toolbar背景色-->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <!--各控件被勾选火选定的颜色-->
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- 默认主题 -->
    <style name="AppTheme" parent="AppTheme.base">
        <item name="windowActionModeOverlay">@color/transparent</item>
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <!--Activity默认的背景色-->
        <item name="android:windowBackground">@color/app_background_color</item>
        <item name="android:fitsSystemWindows">false</item>
    </style>

    <!--透明背景主题-->
    <style name="DCAppTheme.Translucent" parent="Theme.AppCompat.NoActionBar">
        <!--Activity默认的背景色-->
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:windowBackground">@color/transparent</item>
    </style>

    <!--微信登录和支付的主题-->
    <style name="DCAppTheme.WXTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowDisablePreview">true</item>
    </style>

    <!--评论框的主题-->
    <style name="DCAppTheme.CommentDialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/app_40_black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowSoftInputMode">adjustResize|stateAlwaysVisible</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:windowEnterAnimation">@anim/anim_bottom_alpha_in</item>
        <item name="android:windowExitAnimation">@anim/anim_bottom_alpha_out</item>
    </style>

    <style name="AppThemeNoTrans" parent="AppTheme.base">
        <item name="android:windowAnimationStyle">@style/Animation</item>
    </style>

    <style name="Animation">
        <item name="android:activityOpenEnterAnimation">@null</item>
        <item name="android:activityOpenExitAnimation">@null</item>
        <item name="android:activityCloseEnterAnimation">@null</item>
        <item name="android:activityCloseExitAnimation">@null</item>
        <item name="android:taskOpenEnterAnimation">@null</item>
        <item name="android:taskOpenExitAnimation">@null</item>
        <item name="android:taskCloseEnterAnimation">@null</item>
        <item name="android:taskCloseExitAnimation">@null</item>
        <item name="android:taskToFrontEnterAnimation">@null</item>
        <item name="android:taskToFrontExitAnimation">@null</item>
        <item name="android:taskToBackEnterAnimation">@null</item>
        <item name="android:taskToBackExitAnimation">@null</item>
    </style>


    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>


    <style name="AppActionBar" parent="ThemeOverlay.AppCompat.ActionBar">
        <!-- android:textColorPrimary is the color of the title text in the Toolbar  -->
        <item name="android:textColorPrimary">@android:color/white</item>
        <!-- actionMenuTextColor is the color of the text of action (menu) items  -->
        <item name="actionMenuTextColor">@android:color/white</item> <!--控制左侧menu的颜色-->
        <!-- Tints the input fields like checkboxes and text fields -->
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppCompatToolBar" parent="Base.Widget.AppCompat.Toolbar">
        <item name="android:textColorPrimary">@color/bg_white</item>
        <item name="android:textSize">@dimen/t16sp</item>   <!--控制左侧menu的字体大小-->
        <item name="android:textColor">@color/bg_white</item>
        <!--<item name="contentInsetStartWithNavigation">0dp</item>-->
        <!--<item name="contentInsetStart">0dp</item>-->
        <!--<item name="android:customNavigationLayout">@layout/view_toolbar_navigation</item>-->
        <!--<item name="toolbarNavigationButtonStyle">@style/myToolbarNavigationButtonStyle</item>-->
    </style>

    <style name="myToolbarNavigationButtonStyle" parent="@style/Widget.AppCompat.Toolbar.Button.Navigation">
        <!--<item name="android:minWidth">0dp</item>-->
        <!--<item name="android:padding">@dimen/t4dp</item>-->
        <!--<item name="android:scaleType">centerInside</item>-->
    </style>

    <!--toolbar的弹出菜单样式-->
    <style name="ToolbarPopupTheme" parent="@style/ThemeOverlay.AppCompat.ActionBar">
        <item name="android:colorBackground">@color/bg_white</item>
        <item name="android:textColor">@color/bg_black</item>
        <item name="android:gravity">center</item><!--文字居中-->
        <item name="overlapAnchor">false</item>  <!--把该属性改为false即可使menu位置位于toolbar之下-->
    </style>

